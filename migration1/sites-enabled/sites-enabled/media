
server {


	root /var/www/html/leoprada;

	index media.html;

	server_name media.leoprada.com.ar www.media.leoprada.com.ar; 

		location / {
		try_files $uri $uri/  =404;
	}

	location ~ \.php$ {
		include snippets/fastcgi-php.conf;
		fastcgi_pass unix:/run/php/php7.0-fpm.sock;
	}

	location ~ /\.ht {
		deny all;
	}
















    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/meetleoscontacts.leoprada.com.ar/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/meetleoscontacts.leoprada.com.ar/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot




}

server {
    if ($host = media.leoprada.com.ar) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = www.media.leoprada.com.ar) {
        return 301 https://$host$request_uri;
    } # managed by Certbot



	server_name media.leoprada.com.ar www.media.leoprada.com.ar;
    listen 80;
    return 404; # managed by Certbot




}