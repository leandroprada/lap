
server {


	root /var/www/html/mursa;

	index index.html index.php index.htm;

	server_name mursa.leoprada.com.ar www.mursa.leoprada.com.ar; 

		location / {
		try_files $uri $uri/  =404;
	}

	location ~ \.php$ {
		include snippets/fastcgi-php.conf;
		fastcgi_pass unix:/run/php/php7.0-fpm.sock;
	}

	location ~ /\.ht {
		deny all;
	}
















    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/meetleoscontacts.leoprada.com.ar/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/meetleoscontacts.leoprada.com.ar/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot








}

server {
    if ($host = www.mursa.net.ar) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = mursa.net.ar) {
        return 301 https://$host$request_uri;
    } # managed by Certbot



	server_name mursa.net.ar www.mursa.net.ar;
    listen 80;
    return 404; # managed by Certbot




}
