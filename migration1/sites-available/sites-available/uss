
server {


	root /var/www/html/uss;

	index index.html index.php index.htm index.nginx-debian.html blog blog.html ;

	server_name uss.leoprada.com.ar www.uss.leoprada.com.ar fsa.leoprada.com.ar www.fsa.leoprada.com.ar; 

		location / {
		try_files $uri $uri/  =404;
	}

	location ~ \.php$ {
		include snippets/fastcgi-php.conf;
		fastcgi_pass unix:/run/php/php7.0-fpm.sock;
	}

	location ~ /\.ht {
		deny all;
	}




















}
